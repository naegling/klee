    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build-Debug/bin/klee",
            "args": [ "--libc=uclibc", "--posix-runtime", "--libcxx", "RaceCondition2.bc" ],
            "stopAtEntry": false,
            "cwd": "/home/rrutledge/projects/klokkos/DataRaceDetection/IntraKernel/Examples",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "CMake: build"
        }

    ]
}